.container {
  display: flex;
  flex-direction: column;
  padding: 20px;
}

.header {
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 20px;
}

.dishes {
  margin-bottom: 20px;
}

.dish {
  display: flex;
  align-items: center;
  margin-bottom: 10px;
}

.dish image {
  width: 50px;
  height: 50px;
  margin-right: 10px;
}

.dish button {
  margin-left: 10px;
}